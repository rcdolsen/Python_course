[31m-[m
[31m-[m
[31m-PYTHON_SHEBANG_REGEX = re.compile(r'^#!.*\bpython[23]?\b\s*$')[m
[31m-LAMBDA_REGEX = re.compile(r'([\w.]+)\s=\slambda\s*([)(=\w,\s.]*):')[m
[31m-COMPARE_NEGATIVE_REGEX = re.compile(r'\b(not)\s+([^][)(}{]+?)\s+(in|is)\s')[m
[31m-COMPARE_NEGATIVE_REGEX_THROUGH = re.compile(r'\b(not\s+in|is\s+not)\s')[m
[31m-BARE_EXCEPT_REGEX = re.compile(r'except\s*:')[m
[31m-STARTSWITH_DEF_REGEX = re.compile(r'^(async\s+def|def)\s.*\):')[m
[31m-DOCSTRING_START_REGEX = re.compile(r'^u?r?(?P<kind>["\']{3})')[m
[31m-ENABLE_REGEX = re.compile(r'# *(fmt|autopep8): *on')[m
[31m-DISABLE_REGEX = re.compile(r'# *(fmt|autopep8): *off')[m
[31m-[m
[31m-EXIT_CODE_OK = 0[m
[31m-EXIT_CODE_ERROR = 1[m
[31m-EXIT_CODE_EXISTS_DIFF = 2[m
[31m-EXIT_CODE_ARGPARSE_ERROR = 99[m
