     """Parses an editable requirement into:[m
         - a requirement name[m
[36m@@ -504,3 +530,47 @@[m [mdef install_req_from_link_and_ireq([m
         config_settings=ireq.config_settings,[m
         user_supplied=ireq.user_supplied,[m
     )[m
[32m+[m
[32m+[m
[32m+[m[32mdef install_req_drop_extras(ireq: InstallRequirement) -> InstallRequirement:[m
[32m+[m[32m    """[m
[32m+[m[32m    Creates a new InstallationRequirement using the given template but without[m
[32m+[m[32m    any extras. Sets the original requirement as the new one's parent[m
[32m+[m[32m    (comes_from).[m
[32m+[m[32m    """[m
[32m+[m[32m    return InstallRequirement([m
[32m+[m[32m        req=([m
[32m+[m[32m            _set_requirement_extras(ireq.req, set()) if ireq.req is not None else None[m
[32m+[m[32m        ),[m
[32m+[m[32m        comes_from=ireq,[m
[32m+[m[32m        editable=ireq.editable,[m
[32m+[m[32m        link=ireq.link,[m
[32m+[m[32m        markers=ireq.markers,[m
[32m+[m[32m        use_pep517=ireq.use_pep517,[m
[32m+[m[32m        isolated=ireq.isolated,[m
[32m+[m[32m        global_options=ireq.global_options,[m
[32m+[m[32m        hash_options=ireq.hash_options,[m
[32m+[m[32m        constraint=ireq.constraint,[m
[32m+[m[32m        extras=[],[m
[32m+[m[32m        config_settings=ireq.config_settings,[m
[32m+[m[32m        user_supplied=ireq.user_supplied,[m
[32m+[m[32m        permit_editable_wheels=ireq.permit_editable_wheels,[m
[32m+[m[32m    )[m
