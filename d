 [m
[32m+[m[32mdef _set_requirement_extras(req: Requirement, new_extras: Set[str]) -> Requirement:[m
[32m+[m[32m    """[m
[32m+[m[32m    Returns a new requirement based on the given one, with the supplied extras. If the[m
[32m+[m[32m    given requirement already has extras those are replaced (or dropped if no new extras[m
[32m+[m[32m    are given).[m
[32m+[m[32m    """[m
[32m+[m[32m    match: Optional[re.Match[str]] = re.fullmatch([m
[32m+[m[32m        # see https://peps.python.org/pep-0508/#complete-grammar[m
[32m+[m[32m        r"([\w\t .-]+)(\[[^\]]*\])?(.*)",[m
[32m+[m[32m        str(req),[m
[32m+[m[32m        flags=re.ASCII,[m
[32m+[m[32m    )[m
[32m+[m[32m    # ireq.req is a valid requirement so the regex should always match[m
[32m+[m[32m    assert ([m
[32m+[m[32m        match is not None[m
[32m+[m[32m    ), f"regex match on requirement {req} failed, this should never happen"[m
[32m+[m[32m    pre: Optional[str] = match.group(1)[m
[32m+[m[32m    post: Optional[str] = match.group(3)[m
[32m+[m[32m    assert ([m
[32m+[m[32m        pre is not None and post is not None[m
[32m+[m[32m    ), f"regex group selection for requirement {req} failed, this should never happen"[m
[32m+[m[32m    extras: str = "[%s]" % ",".join(sorted(new_extras)) if new_extras else ""[m
[32m+[m[32m    return Requirement(f"{pre}{extras}{post}")[m
[32m+[m
[32m+[m
