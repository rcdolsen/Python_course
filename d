[31m-    else:[m
[31m-        DEFAULT_CONFIG = os.path.join(os.path.expanduser('~/.config'), 'pep8')[m
[31m-PROJECT_CONFIG = ('setup.cfg', 'tox.ini', '.pep8', '.flake8')[m
[31m-[m
[31m-[m
[31m-MAX_PYTHON_FILE_DETECTION_BYTES = 1024[m
[31m-[m
[31m-[m
[31m-def open_with_encoding(filename, mode='r', encoding=None, limit_byte_check=-1):[m
[31m-    """Return opened file with a specific encoding."""[m
[31m-    if not encoding:[m
[31m-        encoding = detect_encoding(filename, limit_byte_check=limit_byte_check)[m
[31m-[m
[31m-    return io.open(filename, mode=mode, encoding=encoding,[m
[31m-                   newline='')  # Preserve line endings[m
[31m-[m
[31m-[m
[31m-def detect_encoding(filename, limit_byte_check=-1):[m
[31m-    """Return file encoding."""[m
[31m-    try:[m
