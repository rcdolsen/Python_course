[32m+[m[32m    def _warn_invalid_extras([m
[32m+[m[32m        self,[m
[32m+[m[32m        requested: FrozenSet[str],[m
[32m+[m[32m        valid: FrozenSet[str],[m
[32m+[m[32m    ) -> None:[m
[32m+[m[32m        """Emit warnings for invalid extras being requested.[m
[32m+[m
[32m+[m[32m        This emits a warning for each requested extra that is not in the[m
[32m+[m[32m        candidate's ``Provides-Extra`` list.[m
[32m+[m[32m        """[m
[32m+[m[32m        invalid_extras_to_warn = frozenset([m
[32m+[m[32m            extra[m
[32m+[m[32m            for extra in requested[m
[32m+[m[32m            if extra not in valid[m
[32m+[m[32m            # If an extra is requested in an unnormalized form, skip warning[m
[32m+[m[32m            # about the normalized form being missing.[m
[32m+[m[32m            and extra in self.extras[m
[32m+[m[32m        )[m
[32m+[m[32m        if not invalid_extras_to_warn:[m
[32m+[m[32m            return[m
[32m+[m[32m        for extra in sorted(invalid_extras_to_warn):[m
[32m+[m[32m            logger.warning([m
[32m+[m[32m                "%s %s does not provide the extra '%s'",[m
[32m+[m[32m                self.base.name,[m
[32m+[m[32m                self.version,[m
[32m+[m[32m                extra,[m
[32m+[m[32m            )[m
[32m+[m
[32m+[m[32m    def _calculate_valid_requested_extras(self) -> FrozenSet[str]:[m
[32m+[m[32m        """Get a list of valid extras requested by this candidate.[m
[32m+[m
[32m+[m[32m        The user (or upstream dependant) may have specified extras that the[m
[32m+[m[32m        candidate doesn't support. Any unsupported extras are dropped, and each[m
[32m+[m[32m        cause a warning to be logged here.[m
[32m+[m[32m        """[m
[32m+[m[32m        requested_extras = self.extras.union(self._unnormalized_extras)[m
[32m+[m[32m        valid_extras = frozenset([m
[32m+[m[32m            extra[m
[32m+[m[32m            for extra in requested_extras[m
[32m+[m[32m            if self.base.dist.is_extra_provided(extra)[m
[32m+[m[32m        )[m
[32m+[m[32m        self._warn_invalid_extras(requested_extras, valid_extras)[m
[32m+[m[32m        return valid_extras[m
